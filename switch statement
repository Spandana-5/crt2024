finding the switch which is multi decision
#include<stdio.h>
int main(){
    int a;
    scanf("%d",&a);
    switch(a){
        case 1:
        printf("java");
        break;
        case 2:
        printf("python");
        break;
        case 3:
        printf(".net");
        default:
        printf("invalid");
        break;
    }
    
    
}
    

