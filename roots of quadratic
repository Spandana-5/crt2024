finding the roots quadratic equation
#include <stdio.h>
#include<math.h>

int main() {
    float a,b,c;
    scanf("%f%f%f",&a,&b,&c);
    float d=b*b-4*a*c;
    if(d<0){
        printf("roots are imaginary");
        
    }
    else{
        float r1=(-b+sqrt(d))/(2*a);
        float r2=(-b-sqrt(d))/(2*a);
        printf("%f %f",r1,r2);
        
    }
    return 0;
}
    
