finding the midpoint
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
    int x,y,z,w;
    scanf("%d%d%d%d",&x,&y,&z,&w);
    float mid=(float)(x+z)/2;
    float mid1=(float)(y+w)/2;
    printf("Binoy's house is located at (%.1f,%.1f)",mid,mid1);
    
    return 0;
}
