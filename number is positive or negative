finding the number is positive or not

#include <stdio.h>
#include<math.h>

int main() {
    int a;
    scanf("%d",&a);
    if(a>0)
    {
        printf("%d is positive number",a);
        
    }
    else if(a<0){
        printf("%d is negative number",a);
    }
    else{
        printf("number is zero ",a);
    }
    return 0;
}
