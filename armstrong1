finding the number of digits which is power of number of digits
#include <stdio.h>


int main() {
    int n,rem,s=0,temp;
    scanf("%d",&n);
    temp=n;
    while(n>=1){
        rem=n%10;
        n=n/10;
        s=s+rem*rem*rem;
        
    }
    if(s==temp){
        printf("armstrong number");
    }
    else{
        printf("not a armstrong number");
    }
}
    
    
