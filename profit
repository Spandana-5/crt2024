calculating profit
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    int t=a*b-a*c;
    int agency=t-100;
    printf("%d",agency);
    return 0;
}
