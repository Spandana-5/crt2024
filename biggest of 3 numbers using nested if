finding the biggest of 3 numbers using nested if

#include <stdio.h>
#include<math.h>

int main() {
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a>b){
        if(a>c){
            printf("%d is biggest",a);
        }
        
            else{
                printf("%d is biggest",c);
            }
    }
         else if(b>c){
                printf("%d is biggest",b);
            }
            else{
                printf("%d is biggest",c);
            }
    }      
